---
import MainLayout from "../layouts/MainLayout.astro";
import Button from '../components/Button.astro';
import Link from '../components/Link.astro';
import { Image } from 'astro:assets';
import thumbsup from "../assets/images/thumbsup.png";
import woman1 from "../assets/images/woman1.png";
import woman2 from "../assets/images/dame2.png";
import woman3 from "../assets/images/dame3.png";
import laura from "../assets/images/mig.png";
import selma from "../assets/images/selma.png";
import ida from "../assets/images/ida.png";
import Steps from "../components/Steps.astro";
import Wave from "../components/Wave.astro";
